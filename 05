function suppliesForSchool(input) {

let numbersOfPens = Number(input[0]);

let numbersOfMarkers = Number(input[1]);

let preparationForBoard = Number(input[2]);

let reduction = Number(input[3]) / 100;

let pensPrice = numbersOfPens * 5.80;
let markersPrice= numbersOfPens * 7.20;
let preparationPrice = preparationForBoard * 1.20;

let priceForAllMaterials = numbersOfPens + numbersOfMarkers + preparationForBoard; 
let discountedPrice = priceForAllMaterials * reduction;

console.log(discountedPrice)
}
suppliesForSchool(["2","3","4","25"])
